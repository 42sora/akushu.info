{
  "name": "aku",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "servefun": "npm --prefix ./functions run serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lintcss": "stylelint \"**/*.vue\" \"**/*.css\" \"**/*.scss\"",
    "fixcss": "stylelint \"**/*.vue\" \"**/*.css\" \"**/*.scss\" --fix",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deployfun": "npm --prefix ./functions run deploy",
    "deploystore": "firebase deploy --only firestore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "firebase": "^6.1.1",
    "firebaseui": "^3.5.2",
    "firebaseui-ja": "^1.0.0",
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.22",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.5",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-pwa": "^3.5.1",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "bulma": "^0.7.4",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
